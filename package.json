  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,md,mdx,css,scss}\"",
    "commit": "git-cz",
    "deploy:preview": "bash scripts/vercel-preview-deploy.sh",
    "deploy:prod": "bash scripts/vercel-prod-deploy.sh",
    "sup:login": "bash scripts/supabase/login.sh",
    "sup:link": "bash scripts/supabase/link.sh",
    "sup:start": "npx supabase start",
    "sup:stop": "npx supabase stop",
    "sup:status": "npx supabase status",
