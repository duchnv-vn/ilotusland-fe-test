    "sup:reset": "bash scripts/supabase/reset.sh",
    "sup:gen-types": "bash scripts/supabase/generate-types.sh",
    "sup:gen-migration": "npx supabase db diff | npx supabase migration new",
    "sup:gen-seed": "npx supabase db dump --data-only -f supabase/seed.sql",
